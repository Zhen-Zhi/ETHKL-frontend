{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"],
      "@components/*": ["./src/components/*"],
      "@asset/*": ["./assets/*"],
      "@constants/*": ["./src/constants/*"],
      "@app/*": ["./src/app/*"],
      "@context/*": ["./src/context/*"]
    }
  },
  
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}


